
<h2>Admin Area</h2>


<%= form_with do |form| %>
<div class="field">

	<%= form.label :name %>
	<div class="input_login">
	<%= form.text_field :name, :size => 50 %>
</div>
</div>

<div class="field">
	<%= form.label :password %>
	<%= form.password_field :password, :size => 50 %>
</div>

<div class="actions">
	<%= form.submit "Login",:class=>"btn btn-success" %>
</div>

<% end %>

